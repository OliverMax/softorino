{
  "compilerOptions": {
    "target": "es2016",
    "module": "Node16",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"],
    }
  },
  "include": ["src/**/*"]
}
